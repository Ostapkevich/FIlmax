<div style="background-color:#f6f1e4 ;" >
    <div class="row top center-align ">
        <div class="col s12 center-align" style="color:#009688 ;">
         <b>Доброго дня, {{nick}}!</b>   
        </div>
    </div>
    <div class="row ">
        <div class="col s12 ">
            <ul class="tabs " style="background-color:#f6f1e4 ;">
                <li class="tab  col s5 offset-s1 "><a class="active " href="#ticket">
                        <h6>Ващі квитки</h6>
                    </a></li>
                <li class="tab col  s5 "><a href="#feedback">
                        <h6>Ваш відгук</h6>
                    </a></li>
            </ul>
            <div class="row"></div>
            <div class="row"></div>  
        </div>
        <div id="ticket" class="col s12 m8 offset-m2">
            <div class=" row">
                <div class="col s12 right-align ">
                    <button class="btn waves-effect waves-light" onclick="print()">Печать <i
                            class="material-icons right">print</i></button>
                </div>
            </div>
            <div class="row">
                <div class="col s12 ">
                    <table>
                        <thead>
                            <tr>
                                <th>Назва фільму</th>
                                <th>Дата сеансу</th>
                                <th>Час</th>
                                <th class="center-align">Номер місця</th>
                                <th class="center-align">Вибір</th>
                            </tr>
                        </thead>
                        <tbody id="tbody">
                            {{#each cont}}
                            <tr>
                                <td>{{filmname}}</td>
                                <td>{{H}}</td>
                                <td>{{T}}</td>
                                <td class="center-align">{{S}}</td>
                                <td class="center-align">
                                    <p><label><input class="with-gap" name="group1" type="radio" /><span></span></label>
                                    </p>
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>

            </div>

            

        </div>
        <div id="feedback" class="col s12 m8 offset-m2">
            <form action="">
                <div class="input-field col s12 ">
                    <i class="material-icons prefix">mode_edit</i>
                    <textarea id="fback" name="feedback" class="materialize-textarea "></textarea>
                    <label for="textarea1">Ваш відгук</label>
                </div>
                <div class="col s12 right-align">
                    <button class="btn waves-effect waves-light " onclick="userFeedback()" name="action">Відправити
                        <i class="material-icons right">send</i>
                    </button>
                </div>
            </form>
        </div>
    </div>

</div>